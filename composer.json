{
    "name": "phpbenchmarks/symfony",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "phpbenchmarks/symfony-common": "4.3.0",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/serializer-pack": "^1.0",
        "symfony/translation": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C08XYTMWXHCW3KKR4WEESEAV",
            "allow-contrib": false
        }
    }
}
