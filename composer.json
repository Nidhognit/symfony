{
    "name": "phpbenchmarks/symfony",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "4.3.0-BETA1",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.3.0-BETA1",
        "symfony/yaml": "4.3.0-BETA1",
        "symfony/routing": "4.3.0-BETA1",
        "symfony/http-foundation": "4.3.0-BETA1",
        "symfony/event-dispatcher": "4.3.0-BETA1",
        "symfony/debug": "4.3.0-BETA1",
        "symfony/http-kernel": "4.3.0-BETA1",
        "symfony/finder": "4.3.0-BETA1",
        "symfony/filesystem": "4.3.0-BETA1",
        "symfony/dependency-injection": "4.3.0-BETA1",
        "symfony/config": "4.3.0-BETA1",
        "symfony/var-exporter": "4.3.0-BETA1",
        "symfony/cache": "4.3.0-BETA1",
        "symfony/mime": "4.3.0-BETA1",
        "phpbenchmarks/symfony-common": "4.1.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.0-BETA1"
        }
    }
}
